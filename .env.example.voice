# Voice Assistant Configuration
# Copie este arquivo para .env e preencha com suas chaves

# OpenAI API Key (obrigat√≥rio)
# Obtenha em: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=sk-...

# OpenAI Model (opcional)
REACT_APP_OPENAI_MODEL=gpt-4-turbo-preview

# Voice Recognition Language (opcional)
REACT_APP_VOICE_LANGUAGE=pt-BR

# Voice Recognition Settings (opcional)
REACT_APP_VOICE_CONTINUOUS=true
REACT_APP_VOICE_INTERIM_RESULTS=true
REACT_APP_VOICE_MAX_ALTERNATIVES=3

# AI Processing Settings (opcional)
REACT_APP_AI_TEMPERATURE=0.3
REACT_APP_AI_MAX_TOKENS=500
REACT_APP_AI_TIMEOUT=10000

# Feature Flags (opcional)
REACT_APP_VOICE_ENABLED=true
REACT_APP_VOICE_SUGGESTIONS_ENABLED=true
REACT_APP_VOICE_OFFLINE_MODE=false
REACT_APP_VOICE_SPEECH_SYNTHESIS=false

# Analytics (opcional)
REACT_APP_VOICE_ANALYTICS_ENABLED=true
